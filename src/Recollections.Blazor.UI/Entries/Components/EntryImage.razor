@inherits EntryImageModel

<div class="col-sm-4 col-md-3 col-lg-2 image @(PlaceHolder != null ? "image-placeholder" : String.Empty)">
    <a href="@GetLinkUrl()">
        <img src="@Url" class="rounded" />
    </a>
    @if (PlaceHolder != null)
    {
        <span>@PlaceHolder</span>
    }
</div>
