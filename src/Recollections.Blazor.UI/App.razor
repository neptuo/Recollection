<Router AppAssembly="typeof(Program).Assembly">
    <Found Context="routeData">
        <UserState RouteData="@routeData">
            <RouteView RouteData="routeData" />
        </UserState>
    </Found>
    <NotFound>
        <div class="container mt-4">
            <NotFound />
        </div>
    </NotFound>
</Router>